CREATE TABLE `t_manager` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `managerCode` varchar(200) DEFAULT NULL COMMENT '管理员账号',
  `managerPassword` varchar(200) DEFAULT NULL COMMENT '管理员密码',
  `salt` varchar(64) DEFAULT NULL COMMENT '密码加盐',
  `createDateTime` datetime NOT NULL COMMENT '创建时间',
  `updateDateTime` datetime NOT NULL COMMENT '最后一次更新时间',
  `description` varchar(1000) DEFAULT NULL COMMENT '备注',
  PRIMARY KEY (`id`),
  UNIQUE KEY `unique_managerCode` (`managerCode`)
) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8;

